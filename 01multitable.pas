Program 01 ;
var num, sequencia, sequencia2: integer;    //contagem sempre integer ou char
Begin
	writeln ('Este programa mostra a tabuada dos números inteiros entre um e dez.');
	writeln ('Pressione "Enter" para dar prosseguimento');
	readln;
	writeln ('Tabuada do um:');
	for sequencia:= 1 to 10 do    //valor inicial e valorfinal
	begin
	writeln (1*sequencia);
	end;  
	writeln ('Tabuada do dois:');
  for sequencia:= 1 to 10 do    //valor inicial e valorfinal
	begin
	writeln (2*sequencia);
	end;
	writeln ('Tabuada do três:');
	for sequencia:= 1 to 10 do    //valor inicial e valorfinal
	begin
	writeln (3*sequencia);
	end;	
	writeln ('Tabuada do quatro:');
	for sequencia:= 1 to 10 do    //valor inicial e valorfinal
	begin	
	writeln (4*sequencia);
	end;	
	writeln ('Tabuada do cinco:');
	for sequencia:= 1 to 10 do    //valor inicial e valorfinal
	begin	
	writeln (5*sequencia);
	end;	
	writeln ('Tabuada do seis:');
	for sequencia:= 1 to 10 do    //valor inicial e valorfinal
	begin	
	writeln (6*sequencia);
	end;	
	writeln ('Tabuada do sete:');
	for sequencia:= 1 to 10 do    //valor inicial e valorfinal
	begin	
	writeln (7*sequencia);
	end;	
	writeln ('Tabuada do oito:');
	for sequencia:= 1 to 10 do    //valor inicial e valorfinal
	begin	
	writeln (8*sequencia);
	end;	
	writeln ('Tabuada do nove:');
	for sequencia:= 1 to 10 do    //valor inicial e valorfinal
	begin	
	writeln (9*sequencia);
	end;
	writeln ('Tabuada do dez:');
	for sequencia:= 1 to 10 do    //valor inicial e valorfinal
	begin	
	writeln (10*sequencia);
	end;
End.
